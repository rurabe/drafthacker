<table class='table table-striped'>
  <tbody>
    <% @players.each do |s| %>
    <tr>
      <td>
        <span class="label important"><%= s.position %></span>
      </td>
      <td>
         <%= s.first_name %>
      </td>
      <td>
         <%= s.last_name %>
      </td>
      <td>
         <%= s.pro_team %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>